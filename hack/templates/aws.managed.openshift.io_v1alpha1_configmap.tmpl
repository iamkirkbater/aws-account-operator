apiVersion: v1
kind: Template
parameters:
- name: ROOT 
- name: BASE
- name: ACCOUNTLIMIT
metadata:
  name: test-aws-ou-mapping-configmap-template
objects:
- apiVersion: v1
  kind: ConfigMap
  metadata:
    name: aws-account-operator-configmap
    namespace: aws-account-operator
  data:
    root: ${ROOT}
    base: ${BASE}
    account-limit: ${ACCOUNTLIMIT}
    MaxReconciles.account: "1"
    MaxReconciles.accountclaim: "1"
    MaxReconciles.accountpool: "1"
    MaxReconciles.awsfederatedaccountaccess: "1"
    MaxReconciles.awsfederatedrole: "1"
